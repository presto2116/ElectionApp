<div class="container">

  <h1>List of Voters</h1>

  <table class="table table-bordered table-striped">
    <thead>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Address</th>
        <th>City</th>
        <th>State</th>
        <th>Zip</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody class="voter-index">
      <!-- render index partial for voter data -->
      <%= render "index" %>
    </tbody>
  </table>

  <div class="new-voter-button">
    <%= link_to "New Voter", new_voter_path, remote: true, class: "btn btn-primary" %>
  </div>

</div>

<div id="voter-modal" class="modal fade"></div>